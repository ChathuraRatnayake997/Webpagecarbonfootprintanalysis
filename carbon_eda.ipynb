    "axes[0,1].set_xlabel('Carbon Rating')\n",
    "axes[0,1].set_ylabel('CO2 Emissions (grams)')\n",
    "\n",
    "# Average emissions by rating\n",
    "mean_emissions = rating_emissions['mean']\n",
    "axes[1,0].bar(mean_emissions.index, mean_emissions.values, color=colors)\n",
    "axes[1,0].set_title('Average CO2 Emissions by Rating')\n",
    "axes[1,0].set_xlabel('Carbon Rating')\n",
    "axes[1,0].set_ylabel('Average CO2 Emissions (grams)')\n",
    "\n",
    "# Green hosting percentage by rating\n",
    "axes[1,1].bar(rating_green_pct.index, rating_green_pct.values, color='green', alpha=0.7)\n",
    "axes[1,1].set_title('Green Hosting Adoption by Rating')\n",
    "axes[1,1].set_xlabel('Carbon Rating')\n",
    "axes[1,1].set_ylabel('Green Hosting Percentage (%)')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(\"üìà Comprehensive rating analysis visualizations\")"
   ]
