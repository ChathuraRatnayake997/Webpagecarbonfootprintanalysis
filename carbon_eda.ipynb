    "## Size vs Emissions Analysis\n",
    "\n",
    "Investigate the relationship between website size and carbon emissions."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Size vs emissions analysis\n",
    "print(\"=== SIZE VS EMISSIONS ANALYSIS ===\")\n",
    "\n",
    "# Correlation analysis\n",
    "size_emission_corr = df['size_mb'].corr(df['gco2e'])\n",
    "print(f\"Correlation between size and emissions: {size_emission_corr:.3f}\")\n",
    "\n",
    "# Create size categories\n",
    "df['size_category'] = pd.cut(df['size_mb'], \n",
    "                           bins=[0, 1, 3, 10, float('inf')],\n",
    "                           labels=['Small (<1MB)', 'Medium (1-3MB)', \n",
    "                                  'Large (3-10MB)', 'Very Large (>10MB)'])\n",
    "\n",
    "# Analyze by size category\n",
    "size_analysis = df.groupby('size_category').agg({\n",
    "    'gco2e': ['mean', 'median', 'count'],\n",
    "    'green': lambda x: (x == True).mean() * 100\n",
    "}).round(2)\n",
    "\n",
    "size_analysis.columns = ['Mean_Emissions', 'Median_Emissions', 'Count', 'Green_Pct']\n",
    "\n",
    "print(f\"\\nAnalysis by Size Category:\")\n",
    "print(size_analysis)\n",
    "\n",
    "# Find correlation for green vs non-green separately\n",
    "green_corr = df[df.green == True]['size_mb'].corr(df[df.green == True]['gco2e'])\n",
    "non_green_corr = df[df.green == False]['size_mb'].corr(df[df.green == False]['gco2e'])\n",
    "\n",
    "print(f\"\\nüìä SIZE-EMISSION CORRELATION:\")\n",
    "print(f\"   Overall: {size_emission_corr:.3f}\")\n",
    "print(f\"   Green hosting: {green_corr:.3f}\")\n",
    "print(f\"   Non-green hosting: {non_green_corr:.3f}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Visualization: Size vs emissions\n",
    "fig, axes = plt.subplots(2, 2, figsize=(15, 12))\n",
    "\n",
    "# Scatter plot with color coding for green hosting\n",
    "green_data = df[df.green == True]\n",
    "non_green_data = df[df.green == False]\n",
    "\n",
    "axes[0,0].scatter(green_data['size_mb'], green_data['gco2e'], \n",
    "                 alpha=0.6, label='Green Hosting', color='green', s=30)\n",
    "axes[0,0].scatter(non_green_data['size_mb'], non_green_data['gco2e'], \n",
    "                 alpha=0.6, label='Non-Green Hosting', color='red', s=30)\n",
    "axes[0,0].set_xlabel('Website Size (MB)')\n",
    "axes[0,0].set_ylabel('CO2 Emissions (grams)')\n",
    "axes[0,0].set_title('Size vs Emissions (by Hosting Type)')\n",
    "axes[0,0].legend()\n",
    "\n",
    "# Add trend lines\n",
    "z_green = np.polyfit(green_data['size_mb'], green_data['gco2e'], 1)\n",
    "p_green = np.poly1d(z_green)\n",
    "z_non_green = np.polyfit(non_green_data['size_mb'], non_green_data['gco2e'], 1)\n",
    "p_non_green = np.poly1d(z_non_green)\n",
    "\n",
    "x_trend = np.linspace(df['size_mb'].min(), df['size_mb'].max(), 100)\n",
    "axes[0,0].plot(x_trend, p_green(x_trend), 'g--', alpha=0.8, linewidth=2)\n",
    "axes[0,0].plot(x_trend, p_non_green(x_trend), 'r--', alpha=0.8, linewidth=2)\n",
    "\n",
    "# Emissions by size category\n",
    "size_emissions = df.groupby('size_category')['gco2e'].mean()\n",
    "bars = axes[0,1].bar(range(len(size_emissions)), size_emissions.values)\n",
    "axes[0,1].set_xticks(range(len(size_emissions)))\n",
    "axes[0,1].set_xticklabels(size_emissions.index, rotation=45)\n",
    "axes[0,1].set_title('Average Emissions by Size Category')\n",
    "axes[0,1].set_ylabel('Average CO2 Emissions (grams)')\n",
    "\n",
    "# Distribution of website sizes\n",
    "axes[1,0].hist(df['size_mb'], bins=50, alpha=0.7, edgecolor='black')\n",
    "axes[1,0].set_xlabel('Website Size (MB)')\n",
    "axes[1,0].set_ylabel('Frequency')\n",
    "axes[1,0].set_title('Distribution of Website Sizes')\n",
    "\n",
    "# Green hosting percentage by size category\n",
