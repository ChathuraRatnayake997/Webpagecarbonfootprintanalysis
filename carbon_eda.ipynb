   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Green Hosting Impact Analysis\n",
    "\n",
    "Analyze how green hosting affects carbon emissions."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Green hosting analysis\n",
    "print(\"=== GREEN HOSTING ANALYSIS ===\")\n",
    "\n",
    "# Basic statistics\n",
    "green_stats = df.groupby('green').agg({\n",
    "    'gco2e': ['mean', 'median', 'std', 'count'],\n",
    "    'size_mb': 'mean',\n",
    "    'requests': 'mean'\n",
    "}).round(3)\n",
    "\n",
    "print(\"Statistics by Hosting Type:\")\n",
    "print(green_stats)\n",
    "\n",
    "# Calculate impact\n",
    "green_mean = df[df.green == True]['gco2e'].mean()\n",
    "non_green_mean = df[df.green == False]['gco2e'].mean()\n",
    "emission_reduction = ((non_green_mean - green_mean) / non_green_mean) * 100\n",
    "\n",
    "print(f\"\\nüéØ KEY FINDING:\")\n",
    "print(f\"   Green hosting reduces emissions by {emission_reduction:.1f}%\")\n",
    "print(f\"   Average emissions:\")\n",
    "print(f\"   - Green hosting: {green_mean:.2f}g CO2\")\n",
    "print(f\"   - Non-green hosting: {non_green_mean:.2f}g CO2\")\n",
    "print(f\"   - Absolute reduction: {non_green_mean - green_mean:.2f}g CO2\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Visualization: Green vs Non-green hosting emissions\n",
    "fig, axes = plt.subplots(1, 2, figsize=(15, 6))\n",
    "\n",
    "# Box plot\n",
    "df.boxplot(column='gco2e', by='green', ax=axes[0])\n",
    "axes[0].set_title('CO2 Emissions by Hosting Type')\n",
    "axes[0].set_xlabel('Green Hosting')\n",
    "axes[0].set_ylabel('CO2 Emissions (grams)')\n",
    "\n",
    "# Distribution plot\n",
    "green_data = df[df.green == True]['gco2e']\n",
    "non_green_data = df[df.green == False]['gco2e']\n",
    "\n",
    "axes[1].hist(green_data, alpha=0.7, label='Green Hosting', bins=30, color='green')\n",
    "axes[1].hist(non_green_data, alpha=0.7, label='Non-Green Hosting', bins=30, color='red')\n",
    "axes[1].set_title('Emission Distribution by Hosting Type')\n",
    "axes[1].set_xlabel('CO2 Emissions (grams)')\n",
    "axes[1].set_ylabel('Frequency')\n",
    "axes[1].legend()\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(\"üìà Visualization: Emissions comparison between green and non-green hosting\")"
   ]
